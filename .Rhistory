columnmeans <- function(x)
{
nc <- ncol(x)
means <- numeric(nc) ## empty numeric vector
for(i in 1:nc){
means[i] <- mean(x[,i])
}
means
}
airquality
columnmeans(airquality)
columnmeans <- function(x, removeNA = TRUE)
{
nc <- ncol(x)
means <- numeric(nc) ## empty numeric vector
for(i in 1:nc){
means[i] <- mean(x[,i], na.rm = removeNA)
}
means
}
columnmeans(airquality)
